{
  "profiles": {
    "http": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "LogstashEndpoint": "http://localhost:5044"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://0.0.0.0:5047;http://localhost:5047"
    },
    "https": {
      "commandName": "Project",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "LogstashEndpoint": "http://localhost:5044"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7028;http://localhost:5047"
    },
    "Container (Dockerfile)": {
      "commandName": "Docker",
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8182",
        "ASPNETCORE_HTTP_PORTS": "8183",
        "LogstashEndpoint": "http://localhost:5044"
      },
      // "publishAllPorts": true, exposes random port, if use this config run docker ps to find exposed port number
      "useSSL": true
    },
    "WSL": {
      "commandName": "WSL2",
      "launchUrl": "https://localhost:7028",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7028;http://localhost:5047",
        "LogstashEndpoint": "http://localhost:5044"
      },
      "distributionName": ""
    }
  },
  "$schema": "https://json.schemastore.org/launchsettings.json"
}